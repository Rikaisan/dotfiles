{
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.iconTheme": "catppuccin-mocha",
    "editor.matchBrackets": "never",
    "chat.agent.enabled": false,
    "editor.minimap.enabled": false,
    "clangd.onConfigChanged": "restart",
    "clangd.onConfigChanged.forceEnable": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.fontFamily": "'JetBrainsMonoNL NF', FontAwesome, Noto Sans, 'monospace', monospace",
    "editor.fontSize": 12,
    "todo-tree.regex.regex": "(--|//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
    "vscord.status.idle.check": false,
    "vscord.status.idle.enabled": false,
    "vscord.status.problems.enabled": false,
    "vscord.status.details.text.notInFile": "Not in a file :c",
    "vscord.status.state.idle.enabled": false,
    "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "chat.disableAIFeatures": true,
    "terminal.integrated.stickyScroll.enabled": false,
    "zig.zls.enabled": "on",
    "zig.path": "/usr/bin/zig",
    "zig.zls.path": "/usr/bin/zls",
    "lazarusoverlook.love2d.path": "/usr/bin/love",
    "mesonbuild.downloadLanguageServer": true,
    "explorer.confirmPasteNative": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
}